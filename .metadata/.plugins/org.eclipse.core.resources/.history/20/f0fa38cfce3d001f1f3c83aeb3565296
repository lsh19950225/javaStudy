package days07;

import java.util.Random;

public class Ex07 {

	public static void main(String[] args) {
		
		// [로또6/45 게임]
		// (int) (Math.random()*45)+1
		
		//Random rnd = new Random();
		//rnd.nextInt(45)+1;
		
		int lotto;
		for (int i = 1; i <= 6; i++) {
			lotto = (int) (Math.random()*45)+1;
			System.out.printf("[%d]", lotto);
		}
		System.out.println();
		
		
		

	} // main

} // class
