package days07;

public class Ex02_04 {

	public static void main(String[] args) {


		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j < i-1; j++) {
				System.out.print("_");
			}
			for (int j = 1; j < (i-1)-i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}





	} // main

} // class
