 */
public class Ex09 {

	public static void main(String[] args) {

		for (int i = 1; i <= 4; i++) {
			// System.out.println("****");
			for (int j = 1; j <= 4; j++) {
				System.out.print("*");
			}
			System.out.println();
		}

		String star = "*";
		for (int i = 1; i <= 4; i++) {
			System.out.printf("%s", star);
			System.out.println();

		}



	}

}
