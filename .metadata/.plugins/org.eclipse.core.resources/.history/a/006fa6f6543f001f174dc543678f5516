package days09;

public class Ex05_03 {

	public static void main(String[] args) {
		
		// 2차원 배열 : 행, 열
		int [][] m = new int [4][6]; // 24
		
		System.out.println(m.length); // 2차원에서는 행 갯수
		System.out.println(m[0].length); // 2차원에서는 0행의 열 갯수
		System.out.println(m[1].length); // 2차원에서는 0행의 열 갯수
		System.out.println(m[2].length); // 2차원에서는 0행의 열 갯수
		System.out.println(m[3].length); // 2차원에서는 0행의 열 갯수
		
		
		
		
		
		
		
		
		
		
		// 1차원 배열 : 열
		/*
		 int [] m = new int [24];
		 
		System.out.println(m.length); // 1차원 배열크기
		
		for (int i = 0; i < m.length; i++) {
			System.out.printf("m[%d]=%d ", i, m[i]);
		}
		System.out.println();
		*/
		
	} // main

} // class
